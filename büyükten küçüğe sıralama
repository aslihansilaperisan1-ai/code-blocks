#include <stdio.h>
#include <stdlib.h>
int a = 0;
int b = 0;
int c = 0;
int extra = 0;
int main()
{
    printf("Lutfen ilk sayiyi giriniz:     \n");
    scanf("%d", &a);
    printf("Lutfen ikinci sayiyi giriniz:     \n");
    scanf("%d", &b);
    printf("Lutfen ucuncu sayiyi giriniz:     \n");
    scanf("%d", &c);

    if(a > b){
            extra = a;
            a = b;
            b = extra;
    }

    if(a > c){
            extra = a;
            a = c;
            c = extra;
    }

    if(b > c){
            extra = b;
            b = c;
            c = extra;
    }

    printf("Kucukten buyuge siralama:    %d %d %d\n", a,b,c);

    return 0;
}